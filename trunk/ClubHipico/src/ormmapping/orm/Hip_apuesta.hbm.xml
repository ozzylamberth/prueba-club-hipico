<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="orm.Hip_apuesta" table="hip_apuesta" lazy="false">
		<id name="ap_id_apuesta" column="ap_id_apuesta" type="string">
			<generator class="increment">
			</generator>
		</id>
		<property name="ap_monto" column="ap_monto" type="integer" length="100" not-null="true" lazy="false"/>
		<property name="ap_tipo" column="ap_tipo" type="string" length="10" not-null="true" lazy="false"/>
		<many-to-one name="ca_id_caballo" column="ca_id_caballo" class="orm.Hip_caballos" cascade="save-update,lock" unique="true" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
		<many-to-one name="co_id_carrera" column="co_id_carrera" class="orm.Hip_corrida" cascade="save-update,lock" unique="true" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
	</class>
</hibernate-mapping>
